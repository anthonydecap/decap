{
  "id": "smart_processor",
  "type": "SharedSlice",
  "name": "SmartProcessor",
  "description": "SmartProcessor section with animated CPU visualization and feature cards",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default SmartProcessor section with animated lines and CPU",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "placeholder": "Powered by SmartProcessor",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em,hyperlink,heading1,heading2,heading3,heading4,heading5,heading6"
          }
        },
        "subtitle": {
          "type": "StructuredText",
          "config": {
            "label": "Subtitle",
            "placeholder": "Everything you need for intelligent organ control.",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em,hyperlink"
          }
        },
        "show_animation": {
          "type": "Boolean",
          "config": {
            "label": "Show Animation",
            "default_value": true,
            "placeholder_false": "Hide animated visualization",
            "placeholder_true": "Show animated visualization"
          }
        }
      },
      "items": {
        "card_title": {
          "type": "Text",
          "config": {
            "label": "Card Title",
            "placeholder": "Card title"
          }
        },
        "card_description": {
          "type": "StructuredText",
          "config": {
            "label": "Card Description",
            "placeholder": "Card description",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em,hyperlink"
          }
        },
        "card_icon": {
          "type": "Select",
          "config": {
            "label": "Card Icon",
            "placeholder": "Select an icon",
            "default_value": "organ_stops",
            "options": [
              "organ_stops",
              "update",
              "id"
            ]
          }
        },
        "accent_color": {
          "type": "Select",
          "config": {
            "label": "Accent Color",
            "placeholder": "Select accent color",
            "options": [
              "blue",
              "green",
              "purple",
              "orange",
              "red",
              "yellow"
            ]
          }
        }
      }
    }
  ]
}
