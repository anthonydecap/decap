        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "SEO title for blog overview"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "SEO description for blog overview"
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "Meta Image",
          "constraint": { "width": 1200, "height": 630 },
