{
  "id": "success_page",
  "label": "Success Page",
  "format": "page",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": "success"
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Payment Successful",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "success": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "Payment Successful - Thank you for your purchase"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "Thank you for your purchase. Your order has been confirmed."
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "Meta Image",
          "constraint": {
            "width": 2400,
            "height": 1260
          },
          "thumbnails": []
        }
      }
    }
  }
} 